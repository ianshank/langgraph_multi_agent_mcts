# Google ADK Integration Configuration
# Copy this file to .env and fill in your values

# ============================================================================
# Google Cloud Configuration
# ============================================================================

# Your Google Cloud Project ID (required for Vertex AI backend)
GOOGLE_CLOUD_PROJECT=your-project-id

# Google Cloud region for API calls
GOOGLE_CLOUD_LOCATION=us-central1

# Backend selection: ml_dev, vertex_ai, or local
# - ml_dev: Google ML Dev (local development with Google APIs)
# - vertex_ai: Full Vertex AI integration (requires GCP project)
# - local: Local execution without Google Cloud (limited functionality)
ADK_BACKEND=local

# Use Vertex AI for Gemini models (true/false)
# Set to true for vertex_ai backend, false for ml_dev
GOOGLE_GENAI_USE_VERTEXAI=false

# ============================================================================
# Model Configuration
# ============================================================================

# Gemini model identifier
# Options: gemini-2.0-flash-001, gemini-2.5-pro, gemini-1.5-pro, etc.
ROOT_AGENT_MODEL=gemini-2.0-flash-001

# LLM sampling temperature (0.0-1.0)
ADK_TEMPERATURE=0.7

# Maximum agent iterations
ADK_MAX_ITERATIONS=10

# Agent timeout in seconds
ADK_TIMEOUT=300

# ============================================================================
# Agent Features
# ============================================================================

# Enable web search capabilities (required for Academic Research and Deep Search)
ADK_ENABLE_SEARCH=true

# Enable OpenTelemetry tracing
ADK_ENABLE_TRACING=true

# Workspace directory for agent artifacts
ADK_WORKSPACE_DIR=./workspace/adk

# ============================================================================
# Data Science Agent Configuration
# ============================================================================

# BigQuery settings
BIGQUERY_PROJECT_ID=your-project-id
BIGQUERY_DATASET_ID=your-dataset

# AlloyDB settings (optional - only if using AlloyDB)
ALLOYDB_INSTANCE=your-alloydb-instance
ALLOYDB_DATABASE=your-database
ALLOYDB_USER=your-user
ALLOYDB_PASSWORD=your-password

# NL2SQL method: baseline or chase-sql
NL2SQL_METHOD=baseline

# ============================================================================
# Data Engineering Agent Configuration
# ============================================================================

# Dataform repository name
DATAFORM_REPOSITORY_NAME=your-dataform-repo

# Dataform workspace name
DATAFORM_WORKSPACE_NAME=your-workspace

# ============================================================================
# ML Engineering Agent Configuration
# ============================================================================

# Google Cloud Storage bucket for model artifacts
GOOGLE_CLOUD_STORAGE_BUCKET=your-gcs-bucket

# MLE-specific settings
MLE_TASK_DIR=./workspace/adk/tasks
MLE_OUTPUT_DIR=./workspace/adk/outputs

# ============================================================================
# Academic Research Agent Configuration
# ============================================================================

# Citation search start date (YYYY-MM-DD)
CITATION_START_DATE=2023-01-01

# ============================================================================
# Authentication
# ============================================================================

# For local development, authenticate with:
# gcloud auth application-default login

# For production, set service account credentials:
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
